module boolean (
    input a,
    input b,
    input alufn[4],
    output out
  ) {
      
  always {
    out = b00;
    case (c{a,b}) {
      b00:
        out = alufn[0];
      b10:
        out = alufn[1];
      b01:
        out = alufn[2];
      b11:
        out = alufn[3];
    }
    
  }
}
